# AcitcratnaAhtraga account (for EthnoGuessr - anonymity)
Host github-ethno
  HostName github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_ethno
  IdentitiesOnly yes

# Main account (huguesdepingon)
Host github-main
  HostName github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes

# Anonymous account for IQGuessr (AcitcratnaAhtraga)
Host github.com
  HostName github.com
  User git
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_anonymous
  IdentitiesOnly yes

