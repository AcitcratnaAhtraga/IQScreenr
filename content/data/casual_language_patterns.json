    "diversity_correction_max": 0.9,
    "diversity_correction_min": 0.3,
    "notes": {
      "text_speak": "Uses logarithmic scaling: penalty = base * (1 + log(1 + count * factor)). Density-weighted for text length normalization.",
      "capitalization": "Uses quadratic scaling: penalty = base * (ratio^2) for partial errors, max_penalty for all lowercase.",
      "punctuation": "Uses square root scaling: penalty = base * sqrt(count * factor) for diminishing returns.",
      "short_text": "Uses exponential decay: penalty = max * exp(-decay_rate * (word_count - 1)). Reliability increases exponentially with sample size.",
      "diversity_correction": "Uses linear interpolation: factor = max - (max - min) * ((word_count - 1) / 8). Decreases smoothly from 0.9 (1 word) to 0.3 (9 words)."
    }
  }
